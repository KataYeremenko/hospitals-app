<h1> Add Doctor </h1>

<%= form_for @doctor do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>

  <%= f.label :email %>
  <%= f.text_field :email %><br>

  <%= f.label :phone %>
  <%= f.text_field :phone %><br>

  <%= f.label :specialty_id %>
  <%= f.number_field :specialty_id %><br>

  <%= f.label :department_id %>
  <%= f.number_field :department_id %><br>

  <%= f.submit "Create" %>
<% end %>