<h1> Update Hospital </h1>

<%= form_for @hospital do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>

  <%= f.label :email %>
  <%= f.text_field :email %><br>

  <%= f.label :phone %>
  <%= f.text_field :phone %><br>

  <%= f.label :address %>
  <%= f.text_field :address %><br>

  <%= f.submit "Update" %>
<% end %>